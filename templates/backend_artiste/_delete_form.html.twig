<form method="post" action="{{ path('backend_artiste_delete', {'id': artiste.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cet artiste?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ artiste.id) }}">
    <button class="btn btn-danger btn-raised mb-0">Supprimer</button>
</form>
