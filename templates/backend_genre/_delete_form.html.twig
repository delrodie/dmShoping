<form method="post" action="{{ path('backend_genre_delete', {'id': genre.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer ce genre?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ genre.id) }}">
    <button class="btn btn-danger btn-raised mb-0">Supprimer</button>
</form>
