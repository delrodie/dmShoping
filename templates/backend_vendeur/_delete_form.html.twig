<form method="post" action="{{ path('backend_vendeur_delete', {'id': vendeur.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer ce vendeur?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ vendeur.id) }}">
    <button class="btn btn-danger btn-raised mb-0">Supprimer</button>
</form>
