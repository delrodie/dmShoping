{% extends 'base.html.twig' %}

{% block title %}Localite{% endblock %}

{% block body %}
    <h1>Localite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ localite.id }}</td>
            </tr>
            <tr>
                <th>Lieu</th>
                <td>{{ localite.lieu }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ localite.prix }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ localite.statut ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ localite.slug }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('backend_localite_index') }}">back to list</a>

    <a href="{{ path('backend_localite_edit', {'id': localite.id}) }}">edit</a>

    {{ include('backend_localite/_delete_form.html.twig') }}
{% endblock %}
